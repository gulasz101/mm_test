{
    "name": "gulasz101/mm_test",
    "type": "project",
    "description": "Test project, sandbox playground.",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "symfony/console": "^5.1",
        "illuminate/support": "^7",
        "illuminate/http": "^7",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^9.0",
        "phpunit/phpunit": "^9.5",
        "php-parallel-lint/php-parallel-lint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Wojciech Gu≈Ça",
            "email": "gulasz101@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "ecs": "vendor/bin/ecs check src tests",
        "lint": "vendor/bin/parallel-lint --exclude vendor ."
    }
}
